{
  "extends": [
    "config:base",
    "github>aquaproj/aqua-renovate-config#0.1.2"
  ],
  "regexManagers": [
    {
      "fileMatch": ["\\.?aqua\\.ya?ml"],
      "matchStrings": [
        " +(version|ref): ('\")?(?<currentValue>[^'\"\\n]+)('\")? +# renovate: depName=(?<depName>)",
        " +(version|ref): '(?<currentValue>[^'\"\\n]+)' +# renovate: depName=(?<depName>)",
        " +name: ('\")?(?<depName>[^'\" /\\n]+/[^'\" /\\n]+)(/[^'\" /@\\n]+)*@(?<currentValue>[^'\"\\n]+)('\")?"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}

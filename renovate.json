{
  "extends": [
    "config:base",
    "github>aquaproj/aqua-renovate-config#0.1.4"
  ],
  "enabledManagers": ["regex"],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "packageRules": [
    {
      "matchPackageNames": ["kubernetes-sigs/kustomize"],
      "matchManagers": ["regex"],
      "extractVersion": "^kustomize/(?<version>.*)$"
    }
  ]
}

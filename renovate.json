{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["\\.?aqua\\.ya?ml"],
      "matchStrings": [
        " +(version|ref): ('\")?(?<currentValue>[^'\"\\n]*)('\")? +# renovate: depName=(?<depName>)",
        " +name: ('\")?(?<depName>[^'\" /\\n]+/[^'\" /\\n]+)(/[^'\" /@\\n]+)*@(?<currentValue>[^'\"\\n]+)('\")?"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}

{
  "enabledManagers": ["gomod"],
  "postUpdateOptions": ["gomodTidy"],
  "packageRules": [
    {
      "matchManagers": ["gomod"],
      "matchUpdateTypes": ["minor", "patch", "pin"]
    },
    {
      "matchPackagePatterns": ["^go\\.opentelemetry\\.io/otel"],
      "groupName": "go.opentelemetry.io/otel"
    }
  ]
}

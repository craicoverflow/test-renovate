{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["aqua\\.yaml"],
      "matchStrings": [
        "^ +(version|ref): (?<currentValue>[^'\"\\n]*) +# renovate: depName=(?<depName>.*)",
        "(version|ref): \"?(?<currentValue>.*)\"? +# renovate: depName=(?<depName>.*)"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}

{
  "enabledManagers": ["regex"],
  "packageRules": [
    {
      "matchManagers": ["regex"],
      "matchDatasources": ["docker"],
      "versioning": "docker"
    }
  ],
  "regexManagers": [
    {
      "fileMatch": [".circleci/config\\.yml"],
      "matchStrings": ["tag: \"(?<currentValue>.*?)\" # renovate: cimg/go"],
      "depNameTemplate": "cimg/go",
      "datasourceTemplate": "docker"
    }
  ]
}

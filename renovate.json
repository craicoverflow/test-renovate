{
  "extends": [
    "config:base",
    "github>suzuki-shunsuke/test-renovate:file(aqua/.*\\.ya?ml)"
  ],
  "regexManagers": [
    {
      "fileMatch": ["aqua\\.yaml"],
      "matchStrings": [
        " +(version|ref): (?<currentValue>[^'\"\\n]*) +# renovate: depName=(?<depName>.*)"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
